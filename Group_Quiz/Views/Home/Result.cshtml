@model List<Group_Quiz.Models.Question>
@{
    ViewData["Title"] = "Result";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>@ViewData["Title"]</h1>
<div class="container result-container">
    <h2>Quiz Summary</h2>
        <img src="~/images/result-trophy.png" alt="Trophy Image" class="mb-4">
    <h3>Congratulations!</h3>
    <p>You've scored ..%</p>
    <div class="divider"></div>
        div class="score-details">
            <div>
        
        <h4>5</h4>
                <p>Total questions</p>
            </div>
            <div>
                <h4>3</h4>
                <p>Correct</p>
            </div>
            <div>
                <h4>2</h4>
                <p>Wrong</p>
            </div>
    <div class="my-2">
            <a href="@Url.Action("Index")" class="btn btn-custom mb-2 rounded-pill px-5 font-weight-bold">Back To Main Page</a><br>
    </div>
        </div>
   
    
</div>